<div class="student">
  <h3>Name: <%= @student.name %></h3><br>
  <h4>Grade: <%= @student.grade %></h4>
</div><br>
<br><br>
<h3>Meal Card ID: <%#= @meal_card.id %></h3><br>
<h3>Meal Card type: <%#= @meal_card.type %></h3><br>
<br><br>
<h1>Meal Card</h1>
<% @meal_card.each do |card| %>
  <div class="mealcard">
    <p>Meal Card ID Number: <%#= card.id %></p><br>
    <p>Meal Card Type: <%#= card.type %></p><br>
  </div><br>
<br><br>

<h1>Courses</h1>
<% @courses.each do |course| %>
  <div class="course">
    <p>Name: <%= course.name %></p><br>
    <p>Topic: <%= course.topic %></p><br>
  </div><br>
<% end %>
